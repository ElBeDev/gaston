{
  "name": "gaston-assistant-backend",
  "version": "1.0.0",
  "description": "Backend API for Gaston Assistant",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "node src/migrations/runMigration.js",
    "migrate:dry-run": "node src/migrations/runMigration.js --dry-run",
    "migrate:force": "node src/migrations/runMigration.js --force",
    "inspect-data": "node src/migrations/inspectData.js",
    "test:routes": "node src/utils/testRoutes.js",
    "test:crm": "node src/utils/testRoutes.js",
    "test:chat": "node src/utils/testChatRoutes.js",
    "sample:data": "node -e \"const t = require('./src/utils/testRoutes.js'); new t().createSampleData()\""
  },
  "dependencies": {
    "@ricky0123/vad-react": "^0.0.30",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "express-validator": "^7.0.1",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "google-auth-library": "^10.3.0",
    "googleapis": "^152.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "openai": "^4.20.1",
    "puppeteer": "^24.22.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "socket.io": "^4.7.4",
    "tmp": "^0.2.3",
    "whatsapp-web.js": "^1.34.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "axios": "^1.10.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "express",
    "mongodb",
    "openai",
    "api"
  ],
  "author": "Gaston Assistant Team",
  "license": "MIT"
}
